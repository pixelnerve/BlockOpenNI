<OpenNI>
	<Licenses>
		<!-- Add licenses here 
		<License vendor="vendor" key="key"/>
		-->
	</Licenses>
	<Log writeToConsole="true" writeToFile="false">
		<!-- 0 - Verbose, 1 - Info, 2 - Warning, 3 - Error (default) -->
		<LogLevel value="1"/>
		<Masks>
			<Mask name="ALL" on="true"/>
		</Masks>
		<Dumps>
		</Dumps>
	</Log>
	<ProductionNodes>
	
		<!-- HighRes Image -->
	<!--
		<Node type="IR" name="IR1">
			<Configuration>
				<MapOutputMode xRes="1280" yRes="1024" FPS="15"/>
